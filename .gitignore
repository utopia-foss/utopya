# Ignore files created by test system
.coverage
.pytest_cache
.tox
__pycache__
coverage.xml
report.xml

# Ignore packaging-related files and directories
dantro.egg-info

# Ignore documentation build directory and auto-generated API reference
doc/_build
doc/api
doc/**/*.log

